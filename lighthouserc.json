{
  "ci": {
    "collect": {
      "staticDistDir": "./out",
      "url": [
        "http://localhost/index.html",
        "http://localhost/about-us.html",
        "http://localhost/contact-us.html",
        "http://localhost/donate.html",
        "http://localhost/volunteer.html",
        "http://localhost/free-charity-web-hosting.html",
        "http://localhost/privacy-policy.html",
        "http://localhost/blog.html",
        "http://localhost/domains.html",
        "http://localhost/help-for-charities.html"
      ],
      "numberOfRuns": 3
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.9 }],
        "categories:accessibility": ["error", { "minScore": 0.95 }],
        "categories:best-practices": ["warn", { "minScore": 0.9 }],
        "categories:seo": ["error", { "minScore": 0.9 }]
      }
    }
  }
}
